{
  "contextFileName": ["AGENTS.md"],
  "excludeTools": ["run_shell_command(rm -rf)"],
  "authAccept": true,
  "mcpServers": {
    "playwright": {
      "command": "bunx",
      "args": ["@playwright/mcp@latest"]
    },
    "sequential-thinking": {
      "command": "bunx",
      "args": ["@modelcontextprotocol/server-sequential-thinking"]
    },
    "context7": {
      "command": "bunx",
      "args": ["@upstash/context7-mcp"]
    }
  },
  "checkpointing": { "enabled": true }
}
